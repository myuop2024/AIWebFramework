# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/database_name
DB_HOST=localhost
DB_PORT=5432
DB_NAME=aiwebframework
DB_USER=your_username
DB_PASSWORD=your_secure_password

# Session Security
SESSION_SECRET=your-super-secure-session-secret-key-at-least-32-characters-long
SESSION_TTL=604800000

# Server Configuration
NODE_ENV=development
PORT=3100
HOST=0.0.0.0

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173,http://localhost:3100

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
AUTH_RATE_LIMIT_MAX=5
API_RATE_LIMIT_MAX=60

# Security Headers
HELMET_CSP_ENABLED=true
HELMET_HSTS_ENABLED=true
HELMET_HSTS_MAX_AGE=31536000

# File Upload Security
MAX_FILE_SIZE=50MB
ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,application/pdf,text/csv

# Logging
LOG_LEVEL=info
LOG_FILE_PATH=./logs/app.log
ERROR_LOG_PATH=./logs/error.log

# External API Keys (if needed)
OPENAI_API_KEY=your_openai_api_key
GOOGLE_MAPS_API_KEY=your_google_maps_api_key
VITE_HERE_API_KEY=your_here_maps_api_key

# Email Configuration (if needed)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_app_password

# Redis Configuration (if using Redis for sessions)
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=your_redis_password

# JWT Configuration (if using JWT)
JWT_SECRET=your-jwt-secret-key
JWT_EXPIRES_IN=7d

# Encryption Keys
ENCRYPTION_KEY=your-32-character-encryption-key
HASH_SALT_ROUNDS=12

# Feature Flags
ENABLE_REGISTRATION=true
ENABLE_PASSWORD_RESET=true
ENABLE_EMAIL_VERIFICATION=false
ENABLE_TWO_FACTOR_AUTH=false

# Performance Settings
ENABLE_COMPRESSION=true
ENABLE_CACHING=true
CACHE_TTL=3600

# Development Settings
DEBUG_MODE=false
ENABLE_SWAGGER=false
ENABLE_PROFILING=false 